<%= form_for @user do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :password %>
<%= f.text_field :password %>
<%= f.label :password_confirmation %>
<%= f.text_field :password_confirmation %>
<%= f.submit 'create_user' %>
<% end %>

<%= @users.each do |u| %>
<%= u.name %>
<% end %>
